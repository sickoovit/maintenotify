generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum DeviceStatus {
  RECEIVED
  WORKING
  DONE
  DELIVERED
}

model Device {
  id          Int          @id @default(autoincrement())
  name        String
  customer    String
  status      DeviceStatus @default(RECEIVED)
  createdAt   DateTime     @default(now())
  updatedAt   DateTime     @updatedAt
}
